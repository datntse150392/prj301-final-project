<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <filter>
        <filter-name>Adminfilter</filter-name>
        <filter-class>filter.Adminfilter</filter-class>
    </filter>
    <filter>
        <filter-name>User_AccountFilter</filter-name>
        <filter-class>filter.User_AccountFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>User_AccountFilter</filter-name>
        <url-pattern>/cart</url-pattern>
        <url-pattern>/history</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>Adminfilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>controller.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>controller.delete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update</servlet-name>
        <servlet-class>controller.update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateproduct</servlet-name>
        <servlet-class>controller.updateproduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateproduct2</servlet-name>
        <servlet-class>controller.updateproduct2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>update_productcount</servlet-name>
        <servlet-class>controller.update_productcount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>history</servlet-name>
        <servlet-class>controller.history</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_cart</servlet-name>
        <servlet-class>controller.delete_cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>controller.register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateproduct</servlet-name>
        <url-pattern>/updateproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateproduct2</servlet-name>
        <url-pattern>/updateproduct2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>update_productcount</servlet-name>
        <url-pattern>/update_productcount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>history</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_cart</servlet-name>
        <url-pattern>/delete_cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>homepage.jsp</welcome-file>
    </welcome-file-list>
</web-app>
